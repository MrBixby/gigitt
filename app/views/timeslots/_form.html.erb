<%= simple_form_for [@venue, @timeslot] do |f| %>
  <% if @timeslot.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@timeslot.errors.count, "error") %> prohibited this timeslot from being saved:</h2>

      <ul>
      <% @timeslot.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  <%= f.input :date, :as => :date_picker %>
  <%= f.input :start_time, :as => :time_picker, input_html: {data: {date_options: {stepping: 15}}} %>
  <%= f.input :end_time, :as => :time_picker %>
  <%= f.input :openings %>
  <%= f.input :paid_gig %>
  <%= f.input :pay_amount %>
  <%= f.input :notes %>
  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
