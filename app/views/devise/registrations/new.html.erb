<br>
<legend>Sign up</legend>
<%= simple_form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>
<div class="field">
<%= f.input :role, collection: [["fan", "Fan"], ["venue", "Venue Owner"], ["band", "Band Member"]], label_method: :profile, label: "Role" %>
  <%= f.label :profile, "Venue Name" .titleize%>
  <%= f.text_field :profile %>
</div>
    <%= f.input :email %>
    <%= f.input :password %>
    <%= f.button :submit, "Sign up" %></br>
<% end %>
